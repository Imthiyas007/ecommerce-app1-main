{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.js$",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css$",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)\\.(png|jpg|jpeg|gif|svg|ico|json|woff|woff2|ttf|eot)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
